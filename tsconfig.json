{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Modale/Modale02.jsx",
    "components/MenuDrinks/Subheading/SubHeading.jsx",
    "components/MenuDrinks/index.jsx",
    "data/images.jsx",
    ".next/dev/types/**/*.ts"
  ],
  "paths": {
    // Remove any middleware-related paths
  },
  "exclude": [
    "node_modules"
  ]
}
